{
  "extends": "next/core-web-vitals",
  "rules": {
    "semi": ["error", "never"],
    "quotes": ["error", "double"],
    "indent": ["error", 2],
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "never",
      "exports": "never",
      "functions": "never"
    }]
  }
}
